<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <data></data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/item_connect_nordic_cl"
        android:layout_width="match_parent"
        android:background="#578BEC"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:layout_marginBottom="6dp"
        android:layout_marginStart="6dp"
        android:layout_marginEnd="6dp">

        <TextView
            android:id="@+id/item_connect_nordic_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:fontFamily="@font/os_bold"
            android:text="MAC: 32j9:432rde213:D234d31:D2"
            android:textColor="@color/colorWhite"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/item_connect_nordic_description"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="6dp"
            android:fontFamily="@font/os_regular"
            android:text="MAC: 32j9:432rde213:D234d31:D2"
            android:textColor="@color/colorWhite"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/item_connect_nordic_name" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>